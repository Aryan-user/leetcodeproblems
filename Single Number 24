#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
     int n;
    
  
    scanf("%d", &n);
    
    int nums[n];
    
 
    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }
    
    int result = 0;
    for(int i=0;i<n;i++){
result ^= nums[i];
    }

   

    printf("%d\n", result);
    return 0;
}
